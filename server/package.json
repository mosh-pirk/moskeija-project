{
  "name": "moskeija",
  "version": "1.0.0",
  "description": "Project for Accounting and financial in moskeija of Tampere",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "start": "NODE_ENV=production node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "build:ui": "cd ../ui && rm -rf build && npm run build && cp -r build ../server",
    "depoly": "fly deploy",
    "deploy:all": "npm run build:ui && npm run depoly",
    "logs:prod": "fly log",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push"
  },
  "keywords": [
    "moskeija",
    "accounting"
  ],
  "author": "Mohamid Al-tueij",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "mongoose-unique-validator": "^4.0.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.2.16",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.6.1",
    "nodemon": "^1.14.9",
    "supertest": "^6.3.3"
  }
}
